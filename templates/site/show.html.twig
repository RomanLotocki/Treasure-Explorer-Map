{% extends 'base.html.twig' %}

{% block title %}Site{% endblock %}

{% block body %}
    <h1>Site</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ site.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ site.name }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ site.image }}</td>
            </tr>
            <tr>
                <th>Site_description</th>
                <td>{{ site.siteDescription }}</td>
            </tr>
            <tr>
                <th>Site_culture</th>
                <td>{{ site.siteCulture }}</td>
            </tr>
            <tr>
                <th>Site_country</th>
                <td>{{ site.siteCountry }}</td>
            </tr>
            <tr>
                <th>Site_latitude</th>
                <td>{{ site.siteLatitude }}</td>
            </tr>
            <tr>
                <th>Site_longitude</th>
                <td>{{ site.siteLongitude }}</td>
            </tr>
            <tr>
                <th>Site_url</th>
                <td>{{ site.siteUrl }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_site_index') }}">back to list</a>

    <a href="{{ path('app_site_edit', {'id': site.id}) }}">edit</a>

    {{ include('site/_delete_form.html.twig') }}
{% endblock %}
